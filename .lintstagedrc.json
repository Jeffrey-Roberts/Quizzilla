{
  "**/*.{js,jsx,ts,tsx}": [
    "prettier --config .prettierrc.cjs --write '**/*.{ts,tsx,js,jsx}'",
    "eslint . --ext .js,.jsx,.ts,.tsx --fix --max-warnings=0",
    "jest -- --findRelatedTests --passWithNoTests"
  ],
  "*.html": [
    "eslint",
    "prettier --write"
  ],
  "*.scss": "prettier --write"
}
